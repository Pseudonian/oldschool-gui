{
    "name": "oldschool-gui",
    "version": "v0",
    "description": "GUI made for older people to learn about technologies.",
    "browserslist": "> 0.5%, last 2 versions, not dead",
    "source": "index.html",
    "eslintConfig": {
        "env": {
            "browser": true,
            "node": true
        }
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@typescript-eslint/eslint-plugin": "^5.12.0",
        "husky": "^7.0.4",
        "parcel": "^2.6.2",
        "stylelint-config-standard": "^25.0.0",
        "typescript": "^4.5.5"
    },
    "dependencies": {
        "@swc/helpers": "^0.4.3",
        "dot-prop": "^7.2.0"
    },
    "scripts": {
        "lint": "npx eslint . --ext .ts",
        "lint:fix": "npm run lint -- --fix",
        "csslint": "npx stylelint styles.css",
        "csslint:fix": "npx stylelint styles.css --fix",
        "check:tsc": "npx -p typescript tsc",
        "watch:tsc": "npx -p typescript tsc --watch",
        "prepare": "husky install",
        "dependency-test": "npx madge --circular --exclude .d.ts --extensions ts ./src"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Pseudonian/oldschool-gui"
    },
    "keywords": [
        "Oldschool GUI",
        "Oldschool",
        "GUI",
        "Pseudonian",
        "Platonic",
        "Kevin"
    ],
    "author": "Kevin Bunn",
    "license": "MIT",
    "homepage": "https://github.com/Pseudonian/oldschool-gui#readme",
    "contributors": [
        {
            "name": "Andrew Milton",
            "url": "https://github.com/miltonandrew17"
        },
        {
            "name": "Jackson Flowers",
            "url": "https://github.com/jflow21"
        },
        {
            "name": "Michelle Mo",
            "url": "https://github.com/michmomo"
        }
    ]
}
